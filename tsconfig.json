{
    "compilerOptions": {
        "declaration": true, /* 生成相关的 '.d.ts' 文件。 */
        "declarationDir": "./dist/types", /* '.d.ts' 文件输出目录 */
        "jsx": "preserve",/* ts中使用jsx语法编译器会提示错误，需要配置一下jsx语法支持*/
    },
    "include": [
        "./**/*.*",
        "./shims-vue.d.ts", "docs/.vitepress/config.ts", "docs/.vitepress/config.ts",
    ],
    "exclude": [
        "node_modules"
    ],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": "true"
}